{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "log",
  "type": "object",
  "description": "One log from a sensor",
  "properties": {
    "timestamp": {
      "type": "number",
      "description": "Timestamp when the log was taken, in milliseconds from the Epoch"
    },
    "value": {
      "type": "number",
      "description": "The measured value"
    }
  },
  "required": [
    "timestamp",
    "value"
  ]
}
